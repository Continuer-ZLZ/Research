rm-item label=\"申请标题\" prop=\"applyTitle\">\n        <el-input v-model=\"form.applyTitle\" placeholder=\"请输入申请标题\"/>\n      </el-form-item>\n      <el-form-item label=\"申请内容\" prop=\"applyContent\">\n        <el-input type=\"textarea\" v-model=\"form.applyContent\" placeholder=\"请输入申请内容\"/>\n      </el-form-item>\n      <el-form-item label=\"开始日期\" prop=\"startDate\">\n        <el-date-picker\n          v-model=\"form.startDate\"\n          type=\"date\"\n          placeholder=\"选择开始日期\"\n          value-format=\"yyyy-MM-dd\"\n        />\n      </el-form-item>\n      <el-form-item label=\"结束日期\" prop=\"endDate\">\n        <el-date-picker\n          v-model=\"form.endDate\"\n          type=\"date\"\n          placeholder=\"选择结束日期\"\n          value-format=\"yyyy-MM-dd\"\n        />\n      </el-form-item>\n      <el-form-item label=\"项目预算\" prop=\"budgetAmount\">\n        <el-input-number v-model=\"form.budgetAmount\" :precision=\"2\" :step=\"0.1\" :min=\"0\"/>\n      </el-form-item>\n      <el-form-item>\n        <el-button type=\"primary\" @click=\"submitForm('form')\">保存草稿</el-button>\n        <el-button type=\"success\" @click=\"submitApply('form')\">提交申请</el-button>\n        <el-button @click=\"cancel\">取消</el-button>\n      </el-form-item>\n    </el-form>\n  </div>\n</template>\n\n<script>\nimport { saveApply, submitApply } from '@/api/project/apply'\n\nexport default {\n  name: \"ProjectApply\",\n  dicts: ['project_type'],\n  data() {\n    return {\n      form: {\n        projectName: '',\n        projectType: '',\n        applyTitle: '',\n        applyContent: '',\n        startDate: '',\n        endDate: '',\n        budgetAmount: 0,\n      },\n      rules: {\n        projectName: [\n          { required: true, message: '请输入项目名称', trigger: 'blur' }\n        ],\n        projectType: [\n          { required: true, message: '请选择项目类型', trigger: 'change' }\n        ],\n        applyTitle: [\n          { required: true, message: '请输入申请标题', trigger: 'blur' }\n        ],\n        startDate: [\n          { required: true, message: '请选择开始日期', trigger: 'change' }\n        ],\n        endDate: [\n          { required: true, message: '请选择结束日期', trigger: 'change' }\n        ],\n        budgetAmount: [\n          { required: true, message: '请输入项目预算', trigger: 'blur' }\n        ],\n      }\n    };\n  },\n  methods: {\n    submitForm(formName) {\n      this.$refs[formName].validate((valid) => {\n        if (valid) {\n          saveApply(this.form).then(response => {\n            this.$modal.msgSuccess(\"保存成功\");\n            this.cancel();\n          });\n        }\n      });\n    },\n    submitApply(formName) {\n      this.$refs[formName].validate((valid) => {\n        if (valid) {\n          submitApply(this.form).then(response => {\n            this.$modal.msgSuccess(\"提交成功\");\n            this.cancel();\n          });\n        }\n      });\n    },\n    cancel() {\n      this.$router.push({ path: '/project/apply/list' });\n    }\n  }\n}\n</script>\n"]}]}