\" placeholder=\"请输入经费来源\" />-->\r\n    <!--    </el-form-item>-->\r\n    <!--  </el-form>-->\r\n    <!--  <div slot=\"footer\" class=\"dialog-footer\">-->\r\n    <!--    <el-button type=\"primary\" @click=\"submitForm\">确 定</el-button>-->\r\n    <!--    <el-button @click=\"cancel\">取 消</el-button>-->\r\n    <!--  </div>-->\r\n    <!--</el-dialog>-->\r\n    <!-- 添加或修改科研项目对话框 -->\r\n    <el-dialog\r\n      :title=\"title\"\r\n      :visible.sync=\"open\"\r\n      width=\"500px\"\r\n      append-to-body\r\n      custom-class=\"custom-dialog\"\r\n    >\r\n      <el-form\r\n        ref=\"form\"\r\n        :model=\"form\"\r\n        :rules=\"rules\"\r\n        label-width=\"100px\"\r\n        class=\"custom-form\"\r\n      >\r\n        <el-form-item label=\"项目名称\" prop=\"projectName\">\r\n          <el-input\r\n            v-model=\"form.projectName\"\r\n            placeholder=\"请输入项目名称\"\r\n            class=\"custom-input\"\r\n          />\r\n        </el-form-item>\r\n        <el-form-item label=\"项目类型\" prop=\"projectType\">\r\n          <el-select\r\n            v-model=\"form.projectType\"\r\n            placeholder=\"请选择项目类型\"\r\n            class=\"custom-select\"\r\n          >\r\n            <el-option\r\n              v-for=\"dict in dict.type.pm_project_type\"\r\n              :key=\"dict.value\"\r\n              :label=\"dict.label\"\r\n              :value=\"dict.value\"\r\n            />\r\n          </el-select>\r\n        </el-form-item>\r\n        <el-form-item label=\"负责人\" prop=\"principalId\">\r\n          <el-select\r\n            v-model=\"form.principalId\"\r\n            placeholder=\"请选择负责人\"\r\n            filterable\r\n            @focus=\"loadUserList\"\r\n            class=\"custom-select\"\r\n          >\r\n            <el-option\r\n              v-for=\"user in userList\"\r\n              :key=\"user.userId\"\r\n              :label=\"user.nickName\"\r\n              :value=\"user.userId\"\r\n            />\r\n          </el-select>\r\n        </el-form-item>\r\n        <el-form-item label=\"所属部门\" prop=\"departmentId\">\r\n          <el-select\r\n            v-model=\"form.departmentId\"\r\n            placeholder=\"请选择部门\"\r\n            filterable\r\n            @focus=\"loadDeptList\"\r\n            class=\"custom-select\"\r\n          >\r\n            <el-option\r\n              v-for=\"dept in deptList\"\r\n              :key=\"dept.deptId\"\r\n              :label=\"dept.deptName\"\r\n              :value=\"dept.deptId\"\r\n            />\r\n          </el-select>\r\n        </el-form-item>\r\n        <el-form-item\r\n          label=\"项目状态\"\r\n          prop=\"status\"\r\n          v-if=\"this.form.projectId != null\"\r\n        >\r\n          <el-select\r\n            v-model=\"form.status\"\r\n            placeholder=\"请选择项目状态\"\r\n            class=\"custom-select\"\r\n          >\r\n            <el-option\r\n              v-for=\"dict in dict.type.pm_project_status\"\r\n              :key=\"dict.value\"\r\n              :label=\"dict.label\"\r\n              :value=\"dict.value\"\r\n            />\r\n          </el-select>\r\n        </el-form-item>\r\n        <el-form-item label=\"总预算金额\" prop=\"budgetTotal\">\r\n          <el-input\r\n            v-model=\"form.budgetTotal\"\r\n            placeholder=\"请输入总预算金额\"\r\n            class=\"custom-input\"\r\n          />\r\n        </el-form-item>\r\n        <el-form-item label=\"经费来源\" prop=\"fundSource\">\r\n          <el-input\r\n            v-model=\"form.fundSource\"\r\n            placeholder=\"请输入经费来源\"\r\n            class=\"custom-input\"\r\n          />\r\n        </el-form-item>\r\n      </el-form>\r\n      <div slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button type=\"primary\" @click=\"submitForm\">确 定</el-button>\r\n        <el-button @click=\"cancel\">取 消</el-button>\r\n      </div>\r\n    </el-dialog>\r\n\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { listProject, getProject, delProject, addProject, updateProject } from \"@/api/project/project\";\r\nimport {listUser} from \"../../../api/system/user\";\r\nimport {listDept} from \"../../../api/system/dept\";\r\n\r\nexport default {\r\n  name: \"Project\",\r\n  dicts: ['pm_project_type', 'pm_project_status'],\r\n  data() {\r\n    return {\r\n      userList: [], // 用户列表缓存\r\n      deptList: [], // 部门列表缓存\r\n      // 遮罩层\r\n      loading: true,\r\n      // 选中数组\r\n      ids: [],\r\n      // 非单个禁用\r\n      single: true,\r\n      // 非多个禁用\r\n      multiple: true,\r\n      // 显示搜索条件\r\n      showSearch: true,\r\n      // 总条数\r\n      total: 0,\r\n      // 科研项目表格数据\r\n      projectList: [],\r\n      // 弹出层标题\r\n      title: \"\",\r\n      // 是否显示弹出层\r\n      open: false,\r\n      // 查询参数\r\n      queryParams: {\r\n        pageNum: 1,\r\n        pageSize: 10,\r\n        projectCode: null,\r\n        projectName: null,\r\n        projectType: null,\r\n        principalId: null,\r\n        departmentId: null,\r\n        applyTime: null,\r\n        status: null,\r\n      },\r\n      // 表单参数\r\n      form: {},\r\n      // 表单校验\r\n      rules: {\r\n        projectName: [\r\n          { required: true, message: \"项目名称不能为空\", trigger: \"blur\" }\r\n        ],\r\n        projectType: [\r\n          { required: true, message: \"项目类型不能为空\", trigger: \"change\" }\r\n        ],\r\n        principalId: [\r\n          { required: true, message: \"负责人ID不能为空\", trigger: \"blur\" }\r\n        ],\r\n        departmentId: [\r\n          { required: true, message: \"所属部门ID不能为空\", trigger: \"blur\" }\r\n        ],\r\n        status: [\r\n          { required: true, message: \"项目状态不能为空\", trigger: \"change\" }\r\n        ],\r\n        budgetTotal: [\r\n          { required: true, message: \"总预算金额不能为空\", trigger: \"blur\" }\r\n        ],\r\n      }\r\n    };\r\n  },\r\n  created() {\r\n    this.getList();\r\n  },\r\n  methods: {\r\n    // 获取用户名称映射\r\n    getUserName(userId) {\r\n      const user = this.userList.find(u => u.userId === userId)\r\n 