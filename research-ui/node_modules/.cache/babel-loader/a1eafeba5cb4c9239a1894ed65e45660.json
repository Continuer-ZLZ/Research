\n      <!--    v-model=\"queryParams.departmentId\"-->\r\n      <!--    placeholder=\"请输入所属部门ID\"-->\r\n      <!--    clearable-->\r\n      <!--    @keyup.enter.native=\"handleQuery\"-->\r\n      <!--  />-->\r\n      <!--</el-form-item>-->\r\n      <el-form-item label=\"负责人\" prop=\"principalId\">\r\n        <el-select\r\n          v-model=\"form.principalId\"\r\n          placeholder=\"请选择负责人\"\r\n          filterable\r\n          @focus=\"loadUserList\"\r\n        >\r\n          <el-option\r\n            v-for=\"user in userList\"\r\n            :key=\"user.userId\"\r\n            :label=\"user.nickName\"\r\n            :value=\"user.userId\"\r\n          />\r\n        </el-select>\r\n      </el-form-item>\r\n\r\n      <el-form-item label=\"所属部门\" prop=\"departmentId\">\r\n        <el-select\r\n          v-model=\"form.departmentId\"\r\n          placeholder=\"请选择部门\"\r\n          filterable\r\n          @focus=\"loadDeptList\"\r\n        >\r\n          <el-option\r\n            v-for=\"dept in deptList\"\r\n            :key=\"dept.deptId\"\r\n            :label=\"dept.deptName\"\r\n            :value=\"dept.deptId\"\r\n          />\r\n        </el-select>\r\n      </el-form-item>\r\n      <!--<el-form-item label=\"申报时间\" prop=\"applyTime\">-->\r\n      <!--  <el-date-picker clearable-->\r\n      <!--    v-model=\"queryParams.applyTime\"-->\r\n      <!--    type=\"date\"-->\r\n      <!--    value-format=\"yyyy-MM-dd\"-->\r\n      <!--    placeholder=\"请选择申报时间\">-->\r\n      <!--  </el-date-picker>-->\r\n      <!--</el-form-item>-->\r\n      <el-form-item label=\"项目状态\" prop=\"status\">\r\n        <el-select v-model=\"queryParams.status\" placeholder=\"请选择项目状态\" clearable>\r\n          <el-option\r\n            v-for=\"dict in dict.type.pm_project_status\"\r\n            :key=\"dict.value\"\r\n            :label=\"dict.label\"\r\n            :value=\"dict.value\"\r\n          />\r\n        </el-select>\r\n      </el-form-item>\r\n      <el-form-item>\r\n        <el-button type=\"primary\" icon=\"el-icon-search\" size=\"mini\" @click=\"handleQuery\">搜索</el-button>\r\n        <el-button icon=\"el-icon-refresh\" size=\"mini\" @click=\"resetQuery\">重置</el-button>\r\n      </el-form-item>\r\n    </el-form>\r\n\r\n    <el-row :gutter=\"10\" class=\"mb8\">\r\n      <el-col :span=\"1.5\">\r\n        <el-button\r\n          type=\"primary\"\r\n          plain\r\n          icon=\"el-icon-plus\"\r\n          size=\"mini\"\r\n          @click=\"handleAdd\"\r\n          v-hasPermi=\"['project:project:add']\"\r\n        >新增</el-button>\r\n      </el-col>\r\n      <el-col :span=\"1.5\">\r\n        <el-button\r\n          type=\"success\"\r\n          plain\r\n          icon=\"el-icon-edit\"\r\n          size=\"mini\"\r\n          :disabled=\"single\"\r\n          @click=\"handleUpdate\"\r\n          v-hasPermi=\"['project:project:edit']\"\r\n        >修改</el-button>\r\n      </el-col>\r\n      <el-col :span=\"1.5\">\r\n        <el-button\r\n          type=\"danger\"\r\n          plain\r\n          icon=\"el-icon-delete\"\r\n          size=\"mini\"\r\n          :disabled=\"multiple\"\r\n          @click=\"handleDelete\"\r\n          v-hasPermi=\"['project:project:remove']\"\r\n        >删除</el-button>\r\n      </el-col>\r\n      <el-col :span=\"1.5\">\r\n        <el-button\r\n          type=\"warning\"\r\n          plain\r\n          icon=\"el-icon-download\"\r\n          size=\"mini\"\r\n          @click=\"handleExport\"\r\n          v-hasPermi=\"['project:project:export']\"\r\n        >导出</el-button>\r\n      </el-col>\r\n      <right-toolbar :showSearch.sync=\"showSearch\" @queryTable=\"getList\"></right-toolbar>\r\n    </el-row>\r\n\r\n    <el-table v-loading=\"loading\" :data=\"projectList\" @selection-change=\"handleSelectionChange\">\r\n      <el-table-column type=\"selection\" width=\"45\" align=\"center\" />\r\n      <el-table-column label=\"序号\" align=\"center\" prop=\"projectId\" width=\"55\"/>\r\n      <el-table-column label=\"项目编号\" align=\"center\" prop=\"projectCode\" width=\"105\"/>\r\n      <el-table-column label=\"项目名称\" align=\"center\" prop=\"projectName\" width=\"180\"/>\r\n      <el-table-column label=\"项目类型\" align=\"center\" prop=\"projectType\">\r\n        <template slot-scope=\"scope\">\r\n          <dict-tag :options=\"dict.type.pm_project_type\" :value=\"scope.row.projectType\"/>\r\n        </template>\r\n      </el-table-column>\r\n      <!--<el-table-column label=\"负责人ID\" align=\"center\" prop=\"principalId\" />-->\r\n      <el-table-column label=\"负责人\" align=\"center\" prop=\"principalId\" width=\"110\">\r\n        <template slot-scope=\"scope\">\r\n          {{ getUserName(scope.row.principalId) }}\r\n        </template>\r\n      </el-table-column>\r\n      <!--<el-table-column label=\"所属部门ID\" align=\"center\" prop=\"departmentId\" />-->\r\n      <el-table-column label=\"所属部门\" align=\"center\" prop=\"departmentId\" width=\"110\">\r\n        <template slot-scope=\"scope\">\r\n          {{ getDeptName(scope.row.departmentId) }}\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column label=\"申报时间\" align=\"center\" prop=\"applyTime\" width=\"120\">\r\n        <template slot-scope=\"scope\">\r\n          <span>{{ parseTime(scope.row.applyTime, '{y}-{m}-{d}') }}</span>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column label=\"立项批复时间\" align=\"center\" prop=\"approveTime\" width=\"120\">\r\n        <template slot-scope=\"scope\">\r\n          <span>{{scope.row.approveTime? parseTime(scope.row.approveTime, '{y}-{m}-{d}'):'-' }}</span>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column label=\"项目状态\" align=\"center\" prop=\"status\">\r\n        <template slot-scope=\"scope\">\r\n          <dict-tag :options=\"dict.type.pm_project_status\" :value=\"scope.row.status?scope.row.status:'待申报'\"/>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column label=\"总预算金额\" align=\"center\" prop=\"budgetTotal\" />\r\n      <el-table-column label=\"经费来源\" align=\"center\" prop=\"fundSource\" />\r\n      <el-table-column label=\"操作\" align=\"center\" class-name=\"small-padding fixed-width\">\r\n        <template slot-scope=\"scope\">\r\n          <el-button\r\n            size=\"mini\"\r\n            type=\"text\"\r\n            icon=\"el-icon-edit\"\r\n            @click=\"handleUpdate(scope.row)\"\r\n            v-hasPermi=\"['project:project:edit']\"\r\n          >修改</el-button>\r\n          <el-button\r\n            size=\"mini\"\r\n            type=\"text\"\r\n            icon=\"el-icon-delete\"\r\n            @click=\"handleDelete(scope.row)\"\r\n            v-hasPermi=\"['project:project:remove']\"\r\n          >删除</el-button>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n\r\n    <pagination\r\n      v-show=\"total>0\"\r\n      :total=\"total\"\r\n      :page.sync=\"queryParams.pageNum\"\r\n      :limit.sync=\"queryParams.pageSize\"\r\n      @pagination=\"getList\"\r\n    />\r\n\r\n    <!-- 添加或修改科研项目对话框 -->\r\n    <!--<el-dialog :title=\"title\" :visible.sync=\"open\" width=\"500px\" append-to-body>-->\r\n    <!--  <el-form ref=\"form\" :model=\"form\" :rules=\"rules\" label-width=\"80px\">-->\r\n    <!--    <el-form-item label=\"项目名称\" prop=\"projectName\">-->\r\n    <!--      <el-input v-model=\"form.projectName\" placeholder=\"请输入项目名称\" />-->\r\n    <!--    </el-form-item>-->\r\n    <!--    <el-form-item label=\"项目类型\" prop=\"projectType\">-->\r\n    <!--      <el-select v-model=\"form.projectType\" placeholder=\"请选择项目类型\">-->\r\n    <!--        <el-option-->\r\n    <!--          v-for=\"dict in dict.type.pm_project_type\"-->\r\n    <!--          :key=\"dict.value\"-->\r\n    <!--          :label=\"dict.label\"-->\r\n    <!--          :value=\"dict.value\"-->\r\n    <!--        ></el-option>-->\r\n    <!--      </el-select>-->\r\n    <!--    </el-form-item>-->\r\n    <!--    <el-form-item label=\"负责人\" prop=\"principalId\